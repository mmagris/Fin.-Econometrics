data(mtcars)


head(mtcars)

model <- lm(mpg ~ disp+wt, data=mtcars)
#model <- lm(mpg ~ wt, data=mtcars)

#model <- lm(mpg ~ disp+wt+hp, data=mtcars)
#model <- lm(mpg ~ wt+hp, data=mtcars)


summary(model)


library(car)


durbinWatsonTest(model)

plot(summary(model)$res)

bgtest(mpg ~ wt+hp, order=5, data=mtcars)

acf(summary(model)$res)